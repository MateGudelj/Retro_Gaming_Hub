{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "PS1",
        "PS2",
        "PS3",
        "NintendoDS",
        "Wii",
        "Xbox",
        "Xbox360"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "products"
    }
  }
}
